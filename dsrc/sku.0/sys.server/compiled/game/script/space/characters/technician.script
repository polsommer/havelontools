// Script to set a few things on Nym's technicians.
//------------------------------------------------

//------------------------------------------------
// Includes
//------------------------------------------------

include library.chat;

//------------------------------------------------
// Constants
//------------------------------------------------

//------------------------------------------------
// OnAttach
//------------------------------------------------

trigger OnAttach()
{
	setInvulnerable( self, true );
	setName( self, "A Starship Technician" );

	return SCRIPT_CONTINUE;
}

//------------------------------------------------
// OnInitialize
//------------------------------------------------

trigger OnInitialize()
{
	setInvulnerable( self, true );
	setName( self, "A Starship Technician" );

	return SCRIPT_CONTINUE;
}
