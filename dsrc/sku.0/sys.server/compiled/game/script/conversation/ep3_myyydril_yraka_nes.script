			utils.setScriptVar (player, "conversation.ep3_myyydril_yraka_nes.branchId", 39);

			npcStartConversation (player, npc, "ep3_myyydril_yraka_nes", message, responses);
		}
		else
		{
			chat.chat (npc, player, message);
		}

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (ep3_myyydril_yraka_nes_condition_isActiveTaskOne (player, npc))
	{
		//-- NPC: You're back. [Yraka Nes nods her head deeply in respect.] I'm so sorry to be rude, but... we need those webweaver strands. Please find them quickly.
		string_id message = new string_id (c_stringFile, "s_1039");
		chat.chat (npc, player, message);

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (ep3_myyydril_yraka_nes_condition_hasCompletedOther (player, npc))
	{
		//-- NPC: [Yraka Nes nods in kind greeting.] I've never seen your face before. I would have remembered someone with such kind features such as yours. Who are you?
		string_id message = new string_id (c_stringFile, "s_1041");
		int numberOfResponses = 0;

		boolean hasResponse = false;

		//-- PLAYER: I'm just a passer-by. Who are you, though?
		boolean hasResponse0 = false;
		if (ep3_myyydril_yraka_nes_condition__defaultCondition (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse0 = true;
		}

		if (hasResponse)
		{
			int responseIndex = 0;
			string_id responses [] = new string_id [numberOfResponses];

			if (hasResponse0)
				responses [responseIndex++] = new string_id (c_stringFile, "s_1043");

			utils.setScriptVar (player, "conversation.ep3_myyydril_yraka_nes.branchId", 42);

			npcStartConversation (player, npc, "ep3_myyydril_yraka_nes", message, responses);
		}
		else
		{
			chat.chat (npc, player, message);
		}

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (ep3_myyydril_yraka_nes_condition__defaultCondition (player, npc))
	{
		//-- NPC: Oh. [Yraka Nes nods quickly. The twi'lek woman seems very busy.] I'm sorry. I wish I could talk right now. It's been so busy around here lately. You may wish to find Kivvaaa. She patrols our caverns. She may want to speak with you.
		string_id message = new string_id (c_stringFile, "s_1133");
		chat.chat (npc, player, message);

		return SCRIPT_CONTINUE;
	}

	chat.chat (npc, "Error:  All conditions for OnStartNpcConversation were false.");

	return SCRIPT_CONTINUE;
}

