int tatooine_rebel_tier3_handleBranch4 (obj_id player, obj_id npc, string_id response)
{
	//-- [BRANCH NOTE] 
	//-- NPC: %TU!  Explain yourself!  What's this I've heard about you assisting the Empire?

	//-- [RESPONSE NOTE] 
	//-- PLAYER: I don't know what you mean
	if (response == "s_ecb17e60")
	{
		//-- [NOTE] 
		if (tatooine_rebel_tier3_condition__defaultCondition (player, npc))
		{
			doAnimationAction (npc, "slow_down");

			//-- NPC: The Rebel Alliance can't trust just ANYONE, you know.  You could be a spy for the Empire... and the reports we've been receiving about you are making me nervous.
			string_id message = new string_id (c_stringFile, "s_861c21de");
			int numberOfResponses = 0;

			boolean hasResponse = false;

			//-- PLAYER: What do I do?
			boolean hasResponse0 = false;
			if (tatooine_rebel_tier3_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse0 = true;
			}

			if (hasResponse)
			{
				int responseIndex = 0;
				string_id responses [] = new string_id [numberOfResponses];

				if (hasResponse0)
					responses [responseIndex++] = new string_id (c_stringFile, "s_f8b02865");

				utils.setScriptVar (player, "conversation.tatooine_rebel_tier3.branchId", 5);

				npcSpeak (player, message);
				npcSetConversationResponses (player, responses);
			}
			else
			{
				utils.removeScriptVar (player, "conversation.tatooine_rebel_tier3.branchId");

				npcEndConversationWithMessage (player, message);
			}

			return SCRIPT_CONTINUE;
		}

	}

	return SCRIPT_DEFAULT;
}

