trigger OnStartNpcConversation (obj_id player)
{
	obj_id npc = self;

	if (ai_lib.isInCombat (npc) || ai_lib.isInCombat (player))
		return SCRIPT_OVERRIDE;

	//-- [NOTE] 
	if (eisley_thranta_directory_terminal_condition__defaultCondition (player, npc))
	{
		//-- NPC: Welcome to the White Thranta Shipping Company, providing safe and secure shipping galaxy-wide.
		string_id message = new string_id (c_stringFile, "s_3");
		int numberOfResponses = 0;

		boolean hasResponse = false;

		//-- PLAYER: [Deploy Surveillance Virus]
		boolean hasResponse0 = false;
		if (eisley_thranta_directory_terminal_condition_onVirus (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse0 = true;
		}

		//-- PLAYER: [Access Customer Information Directory]
		boolean hasResponse1 = false;
		if (eisley_thranta_directory_terminal_condition__defaultCondition (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse1 = true;
		}

		if (hasResponse)
		{
			int responseIndex = 0;
			string_id responses [] = new string_id [numberOfResponses];

			if (hasResponse0)
				responses [responseIndex++] = new string_id (c_stringFile, "s_45");

			if (hasResponse1)
				responses [responseIndex++] = new string_id (c_stringFile, "s_4");

			utils.setScriptVar (player, "conversation.eisley_thranta_directory_terminal.branchId", 1);

			npcStartConversation (player, npc, "eisley_thranta_directory_terminal", message, responses);
		}
		else
		{
			chat.chat (npc, player, message);
		}

		return SCRIPT_CONTINUE;
	}

	chat.chat (npc, "Error:  All conditions for OnStartNpcConversation were false.");

	return SCRIPT_CONTINUE;
}

