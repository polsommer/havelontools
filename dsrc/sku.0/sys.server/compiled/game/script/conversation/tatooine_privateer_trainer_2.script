// ----------------------------------------------------------------------

boolean tatooine_privateer_trainer_2_condition_isSmugglerAllianceMember (obj_id player, obj_id npc)
{
	// return (hasObjVar( player, "smugglerAlliance.member" ));
	
	return space_flags.isSpaceTrack(player, space_flags.PRIVATEER_TATOOINE);
}

// ----------------------------------------------------------------------

boolean tatooine_privateer_trainer_2_condition_isOnTransQuest (obj_id player, obj_id npc)
{
	//You have the patrol quest but you haven't even gotten to the destroy split yet
	//so you are still on the trans quest:
    if ( space_quest.hasQuest( player, "patrol", "tat_priv_quest_trans" ) )
    	return true;
    
    //You've gotten to the split part, so if you haven't completed this then
    //you are still on the trans quest:
    if (space_quest.hasQuest( player, "assassinate", "tat_priv_quest_trans" ) )
    	return true;
    	
    //You don't have this quest or it's variant, so you are still on this quest:    		
	return false;
}

// ----------------------------------------------------------------------

boolean tatooine_privateer_trainer_2_condition_gettingTransQuestReward (obj_id player, obj_id npc)
{
	//if you haven't won the quest, you're not here for the reward:
	if (!space_quest.hasWonQuestRecursive( player, "patrol", "tat_priv_quest_trans" ) )
		return false;
	//else if you have won it but haven't been rewarded for it, you are ready for the reward:
	else if (!space_quest.hasReceivedReward( player, "patrol", "tat_priv_quest_trans" ) )
		return true;
	//otherwise you are not here for the reward, cause you already got it:
	else
	 return false;
}

// ----------------------------------------------------------------------

boolean tatooine_privateer_trainer_2_condition_failedTransQuest (obj_id player, obj_id npc)
{
	return ( space_quest.hasFailedQuestRecursive( player, "patrol", "tat_priv_quest_trans" ) || 
			 space_quest.hasAbortedQuestRecursive( player, "patrol", "tat_priv_quest_trans" ) );

}

// ----------------------------------------------------------------------

boolean tatooine_privateer_trainer_2_condition_hasCompletedSeries (obj_id player, obj_id npc)
{
	// return space_quest.hasCompletedQuestSeries( player, "tatooine_privateer_tier2" );
	if (space_flags.hasCompletedTierTwo(player) &&
		space_quest.hasWonQuest(player, "assassinate", "tatooine_privateer_tier2_1a") &&
		space_quest.hasWonQuest(player, "assassinate", "tatooine_privateer_tier2_2a") &&
		space_quest.hasWonQuest(player, "assassinate", "tatooine_privateer_tier2_3a") &&
		space_quest.hasWonQuest(player, "escort", "tatooine_privateer_tier2_4a"))
		return true;
		
	return false;
}

