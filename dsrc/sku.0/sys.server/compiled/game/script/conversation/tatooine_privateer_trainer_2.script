boolean tatooine_privateer_trainer_2_condition_hasCompletedTransition (obj_id player, obj_id npc)
{
    //you have completed this quest:
    if ( space_quest.hasCompletedQuestRecursive( player, "patrol", "tat_priv_quest_trans" ))
    {
    	//but you aborted it or failed it so that doesn't count:
    	if (!space_quest.hasWonQuestRecursive( player, "patrol", "tat_priv_quest_trans" ) )
    	     	return false;
    	else
    		return true;
    }
    return false;
}

