			hasResponse1 = true;
		}

		//-- PLAYER: I wish to quit this mission. Please erase my progress from your records.
		boolean hasResponse2 = false;
		if (corvette_sabol_imperial1_condition__defaultCondition (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse2 = true;
		}

		if (hasResponse)
		{
			int responseIndex = 0;
			string_id responses [] = new string_id [numberOfResponses];

			if (hasResponse0)
				responses [responseIndex++] = new string_id (c_stringFile, "s_3955e427");

			if (hasResponse1)
				responses [responseIndex++] = new string_id (c_stringFile, "s_37");

			if (hasResponse2)
				responses [responseIndex++] = new string_id (c_stringFile, "s_40");

			utils.setScriptVar (player, "conversation.corvette_sabol_imperial1.branchId", 14);

			npcStartConversation (player, npc, "corvette_sabol_imperial1", message, responses);
		}
		else
		{
			chat.chat (npc, player, message);
		}

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (corvette_sabol_imperial1_condition__defaultCondition (player, npc))
	{
		corvette_sabol_imperial1_action_clearLocations (player, npc);

		//-- NPC: Idiots! Complete and utter idiots! How can I complete my tasks when my commanding officers are nothing but fools! I... Excuse me. I didn't see you standing there. Well... what do you want? I'm very busy as you can see.
		string_id message = new string_id (c_stringFile, "s_145e07df");
		int numberOfResponses = 0;

		boolean hasResponse = false;

		//-- PLAYER: You said something about tasks.
		boolean hasResponse0 = false;
		if (corvette_sabol_imperial1_condition__defaultCondition (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse0 = true;
		}

		//-- PLAYER: Ah, nothing. My apologies for disturbing you.
		boolean hasResponse1 = false;
		if (corvette_sabol_imperial1_condition__defaultCondition (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse1 = true;
		}

		if (hasResponse)
		{
			int responseIndex = 0;
			string_id responses [] = new string_id [numberOfResponses];

			if (hasResponse0)
				responses [responseIndex++] = new string_id (c_stringFile, "s_3098b584");

			if (hasResponse1)
				responses [responseIndex++] = new string_id (c_stringFile, "s_4c16d4f");

			utils.setScriptVar (player, "conversation.corvette_sabol_imperial1.branchId", 23);

			npcStartConversation (player, npc, "corvette_sabol_imperial1", message, responses);
		}
		else
		{
			chat.chat (npc, player, message);
		}

		return SCRIPT_CONTINUE;
	}

	chat.chat (npc, "Error:  All conditions for OnStartNpcConversation were false.");

	return SCRIPT_CONTINUE;
}

