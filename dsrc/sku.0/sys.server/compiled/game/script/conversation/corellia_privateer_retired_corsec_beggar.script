int corellia_privateer_retired_corsec_beggar_handleBranch1 (obj_id player, obj_id npc, string_id response)
{
	//-- [BRANCH NOTE] 
	//-- NPC: Excuse me. Can you spare a credit for an old veteran CorSec pilot?

	//-- [RESPONSE NOTE] 
	//-- PLAYER: Nope. Can't help you.
	if (response == "s_49eaf107")
	{
		//-- [NOTE] 
		if (corellia_privateer_retired_corsec_beggar_condition__defaultCondition (player, npc))
		{
			//-- NPC: Hmph.
			string_id message = new string_id (c_stringFile, "s_82f0aea5");
			removeObjVar (player, "conversation.corellia_privateer_retired_corsec_beggar.branchId");

			chat.chat (npc, player, message);
			npcEndConversation (player);

			return SCRIPT_CONTINUE;
		}

	}

	//-- [RESPONSE NOTE] 
	//-- PLAYER: Sure.
	if (response == "s_8be640b8")
	{
		//-- [NOTE] 
		if (corellia_privateer_retired_corsec_beggar_condition__defaultCondition (player, npc))
		{
			//-- NPC: Thank you! Times have been really tough since the Empire started losing the war! CorSec's turned upside-down... RSF's been turned upside-down... there's no way for a pilot to make a decent living anymore!
			string_id message = new string_id (c_stringFile, "s_2812a379");
			int numberOfResponses = 0;

			boolean hasResponse = false;

			//-- PLAYER: (Give 25 credits)
			boolean hasResponse0 = false;
			if (corellia_privateer_retired_corsec_beggar_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse0 = true;
			}

			//-- PLAYER: (Give 100 credits)
			boolean hasResponse1 = false;
			if (corellia_privateer_retired_corsec_beggar_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse1 = true;
			}

			//-- PLAYER: Upside-down?
			boolean hasResponse2 = false;
			if (corellia_privateer_retired_corsec_beggar_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse2 = true;
			}

			//-- PLAYER: What makes you think the Empire is losing?
			boolean hasResponse3 = false;
			if (corellia_privateer_retired_corsec_beggar_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse3 = true;
			}

			if (hasResponse)
			{
				int responseIndex = 0;
				string_id responses [] = new string_id [numberOfResponses];

				if (hasResponse0)
					responses [responseIndex++] = new string_id (c_stringFile, "s_e28320c1");

				if (hasResponse1)
					responses [responseIndex++] = new string_id (c_stringFile, "s_19104f66");

				if (hasResponse2)
					responses [responseIndex++] = new string_id (c_stringFile, "s_46663cbe");

				if (hasResponse3)
					responses [responseIndex++] = new string_id (c_stringFile, "s_8252a913");

				setObjVar (player, "conversation.corellia_privateer_retired_corsec_beggar.branchId", 36);

				npcSpeak (player, message);
				npcSetConversationResponses (player, responses);
			}
			else
			{
				removeObjVar (player, "conversation.corellia_privateer_retired_corsec_beggar.branchId");

				chat.chat (npc, player, message);
				npcEndConversation (player);
			}

			return SCRIPT_CONTINUE;
		}

	}

	return SCRIPT_DEFAULT;
}

