int tatooine_privateer_tier4_handleBranch2 (obj_id player, obj_id npc, string_id response)
{
	//-- [BRANCH NOTE] 
	//-- NPC: What brings a pilot like you out this way?

	//-- [RESPONSE NOTE] 
	//-- PLAYER: I'm looking for a mission. Do you have any?
	if (response == "s_c1ff5062")
	{
		//-- [NOTE] 
		if (tatooine_privateer_tier4_condition_canFlyNonTrackDuty (player, npc))
		{
			doAnimationAction (npc, "nod_head_once");

			//-- NPC: Eh, you're in luck. Normally I would send you packing, but I do actually have a couple of duty missions . Do you want me to give you a briefing?
			string_id message = new string_id (c_stringFile, "s_e9cabb66");
			int numberOfResponses = 0;

			boolean hasResponse = false;

			//-- PLAYER: Yes, please.
			boolean hasResponse0 = false;
			if (tatooine_privateer_tier4_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse0 = true;
			}

			if (hasResponse)
			{
				int responseIndex = 0;
				string_id responses [] = new string_id [numberOfResponses];

				if (hasResponse0)
					responses [responseIndex++] = new string_id (c_stringFile, "s_c82e9a2f");

				utils.setScriptVar (player, "conversation.tatooine_privateer_tier4.branchId", 3);

				npcSpeak (player, message);
				npcSetConversationResponses (player, responses);
			}
			else
			{
				utils.removeScriptVar (player, "conversation.tatooine_privateer_tier4.branchId");

				npcEndConversationWithMessage (player, message);
			}

			return SCRIPT_CONTINUE;
		}

		//-- [NOTE] 
		if (tatooine_privateer_tier4_condition__defaultCondition (player, npc))
		{
			doAnimationAction (npc, "wave_on_dismissing");

			//-- NPC: Do I have any missions? Yes. Do I think you can handle them? No. Why don't you go find your mama and quit bothering me.
			string_id message = new string_id (c_stringFile, "s_324779f6");
			utils.removeScriptVar (player, "conversation.tatooine_privateer_tier4.branchId");

			npcEndConversationWithMessage (player, message);

			return SCRIPT_CONTINUE;
		}

	}

	return SCRIPT_DEFAULT;
}

