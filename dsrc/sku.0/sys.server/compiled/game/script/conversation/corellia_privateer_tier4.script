int corellia_privateer_tier4_handleBranch7 (obj_id player, obj_id npc, string_id response)
{
	//-- [BRANCH NOTE] 
	//-- NPC: What brings you to the CorSec, pilot? You'd better not be wasting my time.

	//-- [RESPONSE NOTE] 
	//-- PLAYER: I'm looking for a mission. Do you have any?
	if (response == "s_c1ff5062")
	{
		//-- [NOTE] 
		if (corellia_privateer_tier4_condition_canFlyNonTrackDuty (player, npc))
		{
			doAnimationAction (npc, "nod_head_once");

			//-- NPC: There's always work to do here at CorSec. I have a few elective duty missions. Take a look and let me give you a briefing.
			string_id message = new string_id (c_stringFile, "s_3d991214");
			int numberOfResponses = 0;

			boolean hasResponse = false;

			//-- PLAYER: Alright.
			boolean hasResponse0 = false;
			if (corellia_privateer_tier4_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse0 = true;
			}

			if (hasResponse)
			{
				int responseIndex = 0;
				string_id responses [] = new string_id [numberOfResponses];

				if (hasResponse0)
					responses [responseIndex++] = new string_id (c_stringFile, "s_a490d2c6");

				utils.setScriptVar (player, "conversation.corellia_privateer_tier4.branchId", 8);

				npcSpeak (player, message);
				npcSetConversationResponses (player, responses);
			}
			else
			{
				utils.removeScriptVar (player, "conversation.corellia_privateer_tier4.branchId");

				npcEndConversationWithMessage (player, message);
			}

			return SCRIPT_CONTINUE;
		}

		//-- [NOTE] 
		if (corellia_privateer_tier4_condition__defaultCondition (player, npc))
		{
			doAnimationAction (npc, "shake_head_no");

			//-- NPC: There's always work to do here at CorSec. Unfortunately for you, that work is reserved for CorSec pilots. If you're looking for action, you're going to have to go through your own contacts.
			string_id message = new string_id (c_stringFile, "s_784ca15");
			utils.removeScriptVar (player, "conversation.corellia_privateer_tier4.branchId");

			npcEndConversationWithMessage (player, message);

			return SCRIPT_CONTINUE;
		}

	}

	return SCRIPT_DEFAULT;
}

