trigger OnNpcConversationResponse (string conversationId, obj_id player, string_id response)
{
	if (conversationId != "outbreak_canister_demolition_bothan_delta")
		return SCRIPT_CONTINUE;

	obj_id npc = self;

	int branchId = utils.getIntScriptVar (player, "conversation.outbreak_canister_demolition_bothan_delta.branchId");

	if (branchId == 3 && outbreak_canister_demolition_bothan_delta_handleBranch3 (player, npc, response) == SCRIPT_CONTINUE)
		return SCRIPT_CONTINUE;

	if (branchId == 7 && outbreak_canister_demolition_bothan_delta_handleBranch7 (player, npc, response) == SCRIPT_CONTINUE)
		return SCRIPT_CONTINUE;

	if (branchId == 9 && outbreak_canister_demolition_bothan_delta_handleBranch9 (player, npc, response) == SCRIPT_CONTINUE)
		return SCRIPT_CONTINUE;

	if (branchId == 11 && outbreak_canister_demolition_bothan_delta_handleBranch11 (player, npc, response) == SCRIPT_CONTINUE)
		return SCRIPT_CONTINUE;

	if (branchId == 12 && outbreak_canister_demolition_bothan_delta_handleBranch12 (player, npc, response) == SCRIPT_CONTINUE)
		return SCRIPT_CONTINUE;

	if (branchId == 13 && outbreak_canister_demolition_bothan_delta_handleBranch13 (player, npc, response) == SCRIPT_CONTINUE)
		return SCRIPT_CONTINUE;

	if (branchId == 14 && outbreak_canister_demolition_bothan_delta_handleBranch14 (player, npc, response) == SCRIPT_CONTINUE)
		return SCRIPT_CONTINUE;

	if (branchId == 15 && outbreak_canister_demolition_bothan_delta_handleBranch15 (player, npc, response) == SCRIPT_CONTINUE)
		return SCRIPT_CONTINUE;

	if (branchId == 16 && outbreak_canister_demolition_bothan_delta_handleBranch16 (player, npc, response) == SCRIPT_CONTINUE)
		return SCRIPT_CONTINUE;

	chat.chat (npc, "Error:  Fell through all branches and responses for OnNpcConversationResponse.");

	utils.removeScriptVar (player, "conversation.outbreak_canister_demolition_bothan_delta.branchId");

	return SCRIPT_CONTINUE;
}

