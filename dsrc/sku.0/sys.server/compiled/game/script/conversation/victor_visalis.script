			++numberOfResponses;
			hasResponse = true;
			hasResponse4 = true;
		}

		//-- PLAYER: I found these weird stones.
		boolean hasResponse5 = false;
		if (victor_visalis_condition_CheckforStones (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse5 = true;
		}

		//-- PLAYER: Hey wait! What about my reward for voting for you?
		boolean hasResponse6 = false;
