int corvette_colonel_imperial1_handleBranch3 (obj_id player, obj_id npc, string_id response)
{
	//-- [BRANCH NOTE] 
	//-- NPC: Your service to the Empire has been adequate. I suppose a reward is in order, if you desire one?

	//-- [RESPONSE NOTE] 
	//-- PLAYER: A reward? Definitely. Hand it over.
	if (response == "s_21550a19")
	{
		//-- [NOTE] 
		if (corvette_colonel_imperial1_condition_hasInventorySpace (player, npc))
		{
			corvette_colonel_imperial1_action_giveCorvetteReward (player, npc);

			//-- NPC: I thought as much. Very well, take it. You did earn this, though I'd hoped the honor of serving the Empire would have been enough.
			string_id message = new string_id (c_stringFile, "s_81ff39a5");
			utils.removeScriptVar (player, "conversation.corvette_colonel_imperial1.branchId");

			npcEndConversationWithMessage (player, message);

			return SCRIPT_CONTINUE;
		}

		//-- [NOTE] 
		if (!corvette_colonel_imperial1_condition_hasInventorySpace (player, npc))
		{
			//-- NPC: Ah yes. I would hand it over, as you ask, but you don't have room to carry it. So you'll have to clear some space before you may take it.
			string_id message = new string_id (c_stringFile, "s_8e06b4e8");
			utils.removeScriptVar (player, "conversation.corvette_colonel_imperial1.branchId");

			npcEndConversationWithMessage (player, message);

			return SCRIPT_CONTINUE;
		}

	}

	//-- [RESPONSE NOTE] 
	//-- PLAYER: I'm not sure. Maybe I'll come back later.
	if (response == "s_a38773c1")
	{
		//-- [NOTE] 
		if (corvette_colonel_imperial1_condition__defaultCondition (player, npc))
		{
			//-- NPC: Exactly... your reward is serving the Empire. But should you change your mind, simply speak with me again.
			string_id message = new string_id (c_stringFile, "s_dc4a40d6");
			utils.removeScriptVar (player, "conversation.corvette_colonel_imperial1.branchId");

			npcEndConversationWithMessage (player, message);

			return SCRIPT_CONTINUE;
		}

	}

	return SCRIPT_DEFAULT;
}

