		if (emp_day_reb_imp_convert_condition_zero (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse9 = true;
		}

		//-- PLAYER: You can stop whining and fight like the rest of us.
		boolean hasResponse10 = false;
		if (emp_day_reb_imp_convert_condition_plus1 (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse10 = true;
		}

		//-- PLAYER: It's men on the ground, just like you, who can do the most.
		boolean hasResponse11 = false;
		if (emp_day_reb_imp_convert_condition_plus1 (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse11 = true;
		}

		//-- PLAYER: Only if you're willing to be blind to the facts.
		boolean hasResponse12 = false;
		if (emp_day_reb_imp_convert_condition_plus2 (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse12 = true;
		}

		//-- PLAYER: Is the Empire protecting all those beings on the Detainee Disks?
		boolean hasResponse13 = false;
		if (emp_day_reb_imp_convert_condition_plus2 (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse13 = true;
		}

		//-- PLAYER: You've got to stop being a kreetle and be a man.
		boolean hasResponse14 = false;
		if (emp_day_reb_imp_convert_condition_plus3 (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse14 = true;
		}

		//-- PLAYER: Tellulon saw the same things and chose this path. He's making a difference right now.
		boolean hasResponse15 = false;
		if (emp_day_reb_imp_convert_condition_plus3 (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse15 = true;
		}

		if (hasResponse)
		{
			int responseIndex = 0;
			string_id responses [] = new string_id [numberOfResponses];

			if (hasResponse0)
				responses [responseIndex++] = new string_id (c_stringFile, "s_160");

			if (hasResponse1)
				responses [responseIndex++] = new string_id (c_stringFile, "s_161");

			if (hasResponse2)
				responses [responseIndex++] = new string_id (c_stringFile, "s_172");

			if (hasResponse3)
				responses [responseIndex++] = new string_id (c_stringFile, "s_173");

			if (hasResponse4)
				responses [responseIndex++] = new string_id (c_stringFile, "s_182");

			if (hasResponse5)
				responses [responseIndex++] = new string_id (c_stringFile, "s_183");

			if (hasResponse6)
				responses [responseIndex++] = new string_id (c_stringFile, "s_186");

			if (hasResponse7)
				responses [responseIndex++] = new string_id (c_stringFile, "s_187");

			if (hasResponse8)
				responses [responseIndex++] = new string_id (c_stringFile, "s_190");

			if (hasResponse9)
				responses [responseIndex++] = new string_id (c_stringFile, "s_191");

			if (hasResponse10)
				responses [responseIndex++] = new string_id (c_stringFile, "s_204");

			if (hasResponse11)
				responses [responseIndex++] = new string_id (c_stringFile, "s_205");

			if (hasResponse12)
				responses [responseIndex++] = new string_id (c_stringFile, "s_208");

			if (hasResponse13)
				responses [responseIndex++] = new string_id (c_stringFile, "s_209");

			if (hasResponse14)
				responses [responseIndex++] = new string_id (c_stringFile, "s_214");

			if (hasResponse15)
				responses [responseIndex++] = new string_id (c_stringFile, "s_215");

			utils.setScriptVar (player, "conversation.emp_day_reb_imp_convert.branchId", 1);

			npcStartConversation (player, npc, "emp_day_reb_imp_convert", message, responses);
		}
		else
		{
			chat.chat (npc, player, message);
		}

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (emp_day_reb_imp_convert_condition_spokenGood (player, npc))
	{
		//-- NPC: Thank you for helping me choose the right path. I hope I get to see you again some day. 
		string_id message = new string_id (c_stringFile, "s_157");
		chat.chat (npc, player, message);

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (emp_day_reb_imp_convert_condition_spokenBad (player, npc))
	{
		//-- NPC: I have nothing else to say to you.
		string_id message = new string_id (c_stringFile, "s_158");
		chat.chat (npc, player, message);

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (!emp_day_reb_imp_convert_condition_canSpeak (player, npc))
	{
		//-- NPC: I'm sorry I can't talk right now.
		string_id message = new string_id (c_stringFile, "s_159");
		chat.chat (npc, player, message);

		return SCRIPT_CONTINUE;
	}

	chat.chat (npc, "Error:  All conditions for OnStartNpcConversation were false.");

	return SCRIPT_CONTINUE;
}

