trigger OnNpcConversationResponse (string conversationId, obj_id player, string_id response)
{
	if (conversationId != "eisley_thranta_durr_terminal")
		return SCRIPT_CONTINUE;

	obj_id npc = self;

	int branchId = utils.getIntScriptVar (player, "conversation.eisley_thranta_durr_terminal.branchId");

	if (branchId == 1 && eisley_thranta_durr_terminal_handleBranch1 (player, npc, response) == SCRIPT_CONTINUE)
