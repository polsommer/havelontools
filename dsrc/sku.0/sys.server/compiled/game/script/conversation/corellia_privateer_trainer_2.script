				hasResponse = true;
				hasResponse0 = true;
			}

			//-- PLAYER: Escort Duty
			boolean hasResponse1 = false;
			if (corellia_privateer_trainer_2_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse1 = true;
			}

			//-- PLAYER: Destroy Duty
			boolean hasResponse2 = false;
			if (corellia_privateer_trainer_2_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse2 = true;
			}

			//-- PLAYER: Recovery Duty
			boolean hasResponse3 = false;
			if (corellia_privateer_trainer_2_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse3 = true;
			}

			if (hasResponse)
			{
				int responseIndex = 0;
				string_id responses [] = new string_id [numberOfResponses];

				if (hasResponse0)
					responses [responseIndex++] = new string_id (c_stringFile, "s_fa5c5669");

				if (hasResponse1)
