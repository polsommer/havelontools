int naboo_privateer_trainer_2_handleBranch4 (obj_id player, obj_id npc, string_id response)
{
	//-- [BRANCH NOTE] 
	//-- NPC: How do you do! Captain Kaydine - the most decorated starfighter pilot in the Royal Security Forces... at your service!

	//-- [RESPONSE NOTE] 
	//-- PLAYER: I'm looking for a mission. Do you have any?
	if (response == "s_c1ff5062")
	{
		//-- [NOTE] 
		if (naboo_privateer_trainer_2_condition_canFlyNonTrackDuty (player, npc))
		{
			//-- NPC: It must be your lucky day. I actually do have some elective duty missions. Take a look and tell me what you want to do.
			string_id message = new string_id (c_stringFile, "s_8a84109d");
			int numberOfResponses = 0;

			boolean hasResponse = false;

			//-- PLAYER: Protect the Royal Kylantha cargo.
			boolean hasResponse0 = false;
			if (naboo_privateer_trainer_2_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse0 = true;
			}

			//-- PLAYER: Recover ships stolen by the Ay'Nat Legion.
			boolean hasResponse1 = false;
			if (naboo_privateer_trainer_2_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse1 = true;
			}

			//-- PLAYER: Destroy Ay'Nat Legion pirates in this system.
			boolean hasResponse2 = false;
			if (naboo_privateer_trainer_2_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse2 = true;
			}

			if (hasResponse)
			{
				int responseIndex = 0;
				string_id responses [] = new string_id [numberOfResponses];

				if (hasResponse0)
					responses [responseIndex++] = new string_id (c_stringFile, "s_9b73da8");

				if (hasResponse1)
					responses [responseIndex++] = new string_id (c_stringFile, "s_30297e22");

				if (hasResponse2)
					responses [responseIndex++] = new string_id (c_stringFile, "s_c892e540");

				utils.setScriptVar (player, "conversation.naboo_privateer_trainer_2.branchId", 55);

				npcSpeak (player, message);
				npcSetConversationResponses (player, responses);
			}
			else
			{
				utils.removeScriptVar (player, "conversation.naboo_privateer_trainer_2.branchId");

				npcEndConversationWithMessage (player, message);
			}

			return SCRIPT_CONTINUE;
		}

		//-- [NOTE] 
		if (naboo_privateer_trainer_2_condition__defaultCondition (player, npc))
		{
			//-- NPC: Sorry, kiddo. You don't have RSF clearance to fly any of my missions. You'd probably have better luck with your own contacts.
			string_id message = new string_id (c_stringFile, "s_7458c693");
			utils.removeScriptVar (player, "conversation.naboo_privateer_trainer_2.branchId");

			npcEndConversationWithMessage (player, message);

			return SCRIPT_CONTINUE;
		}

	}

	return SCRIPT_DEFAULT;
}

