int corellia_rebel_tier3_handleBranch2 (obj_id player, obj_id npc, string_id response)
{
	//-- [BRANCH NOTE] Seems that you are in luck pilot, you're getting transferred. Even though you have been a pain ever since you got here, I suppose you're not that bad of a pilot.
	//-- NPC: All right! I've done the best I can with you. Your transfer has come through. 

	//-- [RESPONSE NOTE] 
	//-- PLAYER: Finally!
	if (response == "s_ad721916")
	{
		doAnimationAction (player, "celebrate");

		//-- [NOTE] Don't get clever with me, there's still time to revoke this transfer! No? Alright, you're going back to Lok %NU. There's a man named Warvog Arkon in Nym's excuse for a hotel. Report to him immediately!
		if (corellia_rebel_tier3_condition__defaultCondition (player, npc))
		{
			doAnimationAction (npc, "pound_fist_palm");

			doAnimationAction (player, "slow_down");

			//-- NPC: Watch your mouth, pilot! You should be thanking your lucky stars that you've been trained by the great Aqzow. 
			string_id message = new string_id (c_stringFile, "s_9795f675");
			int numberOfResponses = 0;

			boolean hasResponse = false;

			//-- PLAYER: Whatever you say, sir.
			boolean hasResponse0 = false;
			if (corellia_rebel_tier3_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse0 = true;
			}

			if (hasResponse)
			{
				int responseIndex = 0;
				string_id responses [] = new string_id [numberOfResponses];

				if (hasResponse0)
					responses [responseIndex++] = new string_id (c_stringFile, "s_8bf9d94d");

				utils.setScriptVar (player, "conversation.corellia_rebel_tier3.branchId", 3);

				npcSpeak (player, message);
				npcSetConversationResponses (player, responses);
			}
			else
			{
				utils.removeScriptVar (player, "conversation.corellia_rebel_tier3.branchId");

				npcEndConversationWithMessage (player, message);
			}

			return SCRIPT_CONTINUE;
		}

	}

	//-- [RESPONSE NOTE] 
	//-- PLAYER: Thank you, sir.
	if (response == "s_987445c9")
	{
		doAnimationAction (player, "salute1");

		//-- [NOTE] Yeah yeah. You're going back to Lok %TU. There's a man named Warvog Arkon in Nym's excuse for a hotel, report to him as soon as you can. Oh and %NU, good luck... Now get out of here!
		if (corellia_rebel_tier3_condition__defaultCondition (player, npc))
		{
			doAnimationAction (npc, "salute1");

			doAnimationAction (player, "salute1");

			//-- NPC: You're welcome. And congratulations, solider. You've been trained by the best. Me! Remember my lessons when your new trainer starts flapping his gums. His name is Warvog Arkon. The man's a fool. You'll find him in Nym's pathetic excuse for a hotel. Dismissed! 
			string_id message = new string_id (c_stringFile, "s_30be37b");
			utils.removeScriptVar (player, "conversation.corellia_rebel_tier3.branchId");

			npcEndConversationWithMessage (player, message);

			return SCRIPT_CONTINUE;
		}

	}

	return SCRIPT_DEFAULT;
}

