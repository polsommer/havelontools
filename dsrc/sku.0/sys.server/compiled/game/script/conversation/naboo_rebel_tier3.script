boolean naboo_rebel_tier3_condition_collectingQuestTwoReward (obj_id player, obj_id npc)
{
	//if the player has WON this quest, but has not been rewarded for it
	//then return TRUE so the NPC can grant the reward!
	if ( !space_quest.hasCompletedQuestRecursive( player, "space_battle", "naboo_rebel_tier3_2"))
		return false;//haven't done it, so must not be here for the reward

	//we assume if you have two skills then you have already been rewarded for this quest
	int skillCount = 0;
	if ( hasSkill( player, "pilot_rebel_navy_starships_03") )
		skillCount++;
		
	if ( hasSkill( player, "pilot_rebel_navy_procedures_03") )
		skillCount++;
	
	if ( hasSkill( player, "pilot_rebel_navy_weapons_03") )
		skillCount++;
	
	if ( hasSkill( player, "pilot_rebel_navy_droid_03" ) )
		skillCount++;
		
	if ( skillCount < 2 )
		return true;//return true because you have less than 2 skills, must not have been rewarded for this
	else
		return false;//you must have already been rewarded for this	
}

