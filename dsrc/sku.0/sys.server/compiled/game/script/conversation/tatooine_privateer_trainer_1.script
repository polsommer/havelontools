int tatooine_privateer_trainer_1_handleBranch1 (obj_id player, obj_id npc, string_id response)
{
	//-- [BRANCH NOTE] 
	//-- NPC: You've got your assignment and the control device for your ship in your datapad. Go to the Starport and access the terminal to launch into space.

	//-- [RESPONSE NOTE] 
	//-- PLAYER: Where do I go now?
	if (response == "s_111621e7")
	{
		doAnimationAction (player, "shrug_hands");

		//-- [NOTE] 
		if (tatooine_privateer_trainer_1_condition__defaultCondition (player, npc))
		{
			doAnimationAction (npc, "explain");

			//-- NPC: Go to the starport. Look for a 'starship terminal.' You can use these terminals to configure your ship and to launch into space.
			string_id message = new string_id (c_stringFile, "s_8622fd74");
			int numberOfResponses = 0;

			boolean hasResponse = false;

			//-- PLAYER: Where do I go now?
			boolean hasResponse0 = false;
			if (tatooine_privateer_trainer_1_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse0 = true;
			}

