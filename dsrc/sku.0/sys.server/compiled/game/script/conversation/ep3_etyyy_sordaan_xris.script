			return false;
		
		if ( groundquests.isTaskActive(player, "ep3_hunt_sordaan_mouf_bet", "sordaan_moufBet") )
			return false;
		
		if ( groundquests.isTaskActive(player, "ep3_hunt_sordaan_webweaver_bet", "sordaan_webweaverBet") )
			return false;
			
		return true;
	}
	return false;
