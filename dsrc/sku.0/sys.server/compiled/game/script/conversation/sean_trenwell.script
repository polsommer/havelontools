					return true;
				}
				if( strItemTemplate=="object/tangible/loot/quest/victor_questn_dseal.iff" )
				{
					destroyObject( objContents[intI] );
					return true;
				}
				if( strItemTemplate=="object/tangible/loot/quest/victor_questn_journal.iff" )
				{
					destroyObject( objContents[intI] );
					return true;
				}			
			
			}
			
		}
	}
