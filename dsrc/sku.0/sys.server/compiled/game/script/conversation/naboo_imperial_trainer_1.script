	faceTo( npc, player );
	doAnimationAction( npc, "conversation_1" );
	
	// Mission grant.
	space_quest.grantQuest( player, "patrol", "naboo_imperial_1" );
	space_flags.restoreClientPath( player );
}

// ----------------------------------------------------------------------

