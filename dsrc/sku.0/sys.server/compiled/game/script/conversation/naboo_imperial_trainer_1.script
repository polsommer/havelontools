		 return true;
	else if (	!hasSkill( player, "pilot_imperial_navy_starships_01" ) &&
		 		!hasSkill( player, "pilot_imperial_navy_weapons_01" ) &&
		 		!hasSkill( player, "pilot_imperial_navy_procedures_01" ) &&
		 		!hasSkill( player, "pilot_imperial_navy_droid_01" ) )
		 return true;
	else
		return false;
}

