trigger OnStartNpcConversation (obj_id player)
{
	obj_id npc = self;

	if (ai_lib.isInCombat (npc) || ai_lib.isInCombat (player))
		return SCRIPT_OVERRIDE;

	//-- [NOTE] 
	if (corsec_captain_condition_finishedCaptain (player, npc))
	{
		corsec_captain_action_sendConvincedSignal (player, npc);

		//-- NPC: No, no. You're still on leave. I can't have you tiring yourself out. The six years you did undercover has earned you a nice long vacation.
		string_id message = new string_id (c_stringFile, "s_26");
		chat.chat (npc, player, message);

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (corsec_captain_condition_evidenceReturned (player, npc))
	{
		//-- NPC: They killed Maerzen? Oh, this is just too much! It's like they know every move we're going to make before we make it. How do they do it?
		string_id message = new string_id (c_stringFile, "s_5");
		int numberOfResponses = 0;

		boolean hasResponse = false;

		//-- PLAYER: Why are you asking me?
		boolean hasResponse0 = false;
		if (corsec_captain_condition__defaultCondition (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse0 = true;
		}

		//-- PLAYER: I can't imagine.
		boolean hasResponse1 = false;
		if (corsec_captain_condition__defaultCondition (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse1 = true;
		}

		//-- PLAYER: Um...let me know if you figure it out.
		boolean hasResponse2 = false;
		if (corsec_captain_condition__defaultCondition (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse2 = true;
		}

		if (hasResponse)
		{
			int responseIndex = 0;
			string_id responses [] = new string_id [numberOfResponses];

			if (hasResponse0)
				responses [responseIndex++] = new string_id (c_stringFile, "s_41");

			if (hasResponse1)
				responses [responseIndex++] = new string_id (c_stringFile, "s_42");

			if (hasResponse2)
				responses [responseIndex++] = new string_id (c_stringFile, "s_43");

			utils.setScriptVar (player, "conversation.corsec_captain.branchId", 2);

			npcStartConversation (player, npc, "corsec_captain", message, responses);
		}
		else
		{
			chat.chat (npc, player, message);
		}

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (corsec_captain_condition_gettingEvidence (player, npc))
	{
		//-- NPC: Where's Maerzen? Where's the evidence? You didn't forget did you?
		string_id message = new string_id (c_stringFile, "s_18");
		chat.chat (npc, player, message);

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (corsec_captain_condition_aldaladKilled (player, npc))
	{
		corsec_captain_action_sendAldaladSignal (player, npc);

		//-- NPC: Aldalad was killed? I can't believe it! I'd say you're pretty lucky.
		string_id message = new string_id (c_stringFile, "s_20");
		int numberOfResponses = 0;

		boolean hasResponse = false;

		//-- PLAYER: Why do you say that...?
		boolean hasResponse0 = false;
		if (corsec_captain_condition__defaultCondition (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse0 = true;
		}

		//-- PLAYER: I can't stand this anymore. I'm done.
		boolean hasResponse1 = false;
		if (corsec_captain_condition__defaultCondition (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse1 = true;
		}

		if (hasResponse)
		{
			int responseIndex = 0;
			string_id responses [] = new string_id [numberOfResponses];

			if (hasResponse0)
				responses [responseIndex++] = new string_id (c_stringFile, "s_27");

			if (hasResponse1)
				responses [responseIndex++] = new string_id (c_stringFile, "s_58");

			utils.setScriptVar (player, "conversation.corsec_captain.branchId", 9);

			npcStartConversation (player, npc, "corsec_captain", message, responses);
		}
		else
		{
			chat.chat (npc, player, message);
		}

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (corsec_captain_condition_gettingAldalad (player, npc))
	{
		//-- NPC: You haven't found him? That's strange, he should be right there where I said. Maybe you should go look again.
		string_id message = new string_id (c_stringFile, "s_62");
		chat.chat (npc, player, message);

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (corsec_captain_condition_hasFakeBadge (player, npc))
	{
		corsec_captain_action_startedWithCaptain (player, npc);

		//-- NPC: Oh...Oh, my! That badge! you're Agent Niksel. You look exactly like I thought you would. So, you've returned!
		string_id message = new string_id (c_stringFile, "s_64");
		int numberOfResponses = 0;

		boolean hasResponse = false;

		//-- PLAYER: Um...yeah, I guess.
		boolean hasResponse0 = false;
		if (corsec_captain_condition__defaultCondition (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse0 = true;
		}

		//-- PLAYER: I need to go get a drink.
		boolean hasResponse1 = false;
		if (corsec_captain_condition__defaultCondition (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse1 = true;
		}

		if (hasResponse)
		{
			int responseIndex = 0;
			string_id responses [] = new string_id [numberOfResponses];

			if (hasResponse0)
				responses [responseIndex++] = new string_id (c_stringFile, "s_66");

			if (hasResponse1)
				responses [responseIndex++] = new string_id (c_stringFile, "s_96");

			utils.setScriptVar (player, "conversation.corsec_captain.branchId", 18);

			npcStartConversation (player, npc, "corsec_captain", message, responses);
		}
		else
		{
			chat.chat (npc, player, message);
		}

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (corsec_captain_condition__defaultCondition (player, npc))
	{
		//-- NPC: I'm a Captain in CorSec. Do you really think I have time to talk to you? Sorry, I don't mean to be rude, I'm just busy.
		string_id message = new string_id (c_stringFile, "s_100");
		chat.chat (npc, player, message);

		return SCRIPT_CONTINUE;
	}

	chat.chat (npc, "Error:  All conditions for OnStartNpcConversation were false.");

	return SCRIPT_CONTINUE;
}

