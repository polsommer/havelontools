trigger OnStartNpcConversation (obj_id player)
{
	obj_id npc = self;

	if (ai_lib.isInCombat (npc) || ai_lib.isInCombat (player))
		return SCRIPT_OVERRIDE;

	//-- [NOTE] 
	if (!naboo_imperial_tier3_barlow_condition_isCorrectFaction (player, npc))
	{
		doAnimationAction (npc, "wave_on_dismissing");

		//-- NPC: This is a restricted area. I suggest that you move along before I am forced to charge you under violation of Imperial directive T43-X9A.
		string_id message = new string_id (c_stringFile, "s_127f71ad");
		chat.chat (npc, player, message);

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (naboo_imperial_tier3_barlow_condition_setByInquisition (player, npc))
	{
		//-- NPC: Yes? What do you want? I am extremely busy.
		string_id message = new string_id (c_stringFile, "s_a72db7d9");
		int numberOfResponses = 0;

		boolean hasResponse = false;

		//-- PLAYER: My name is not important. I am here on behalf of Inquisitor Vrke.
		boolean hasResponse0 = false;
		if (naboo_imperial_tier3_barlow_condition__defaultCondition (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse0 = true;
		}

		//-- PLAYER: I don't care how busy you are. We are going to talk.
		boolean hasResponse1 = false;
		if (naboo_imperial_tier3_barlow_condition__defaultCondition (player, npc))
		{
			++numberOfResponses;
			hasResponse = true;
			hasResponse1 = true;
		}

		if (hasResponse)
		{
			int responseIndex = 0;
			string_id responses [] = new string_id [numberOfResponses];

			if (hasResponse0)
				responses [responseIndex++] = new string_id (c_stringFile, "s_40f102e4");

			if (hasResponse1)
				responses [responseIndex++] = new string_id (c_stringFile, "s_ca59425a");

			setObjVar (player, "conversation.naboo_imperial_tier3_barlow.branchId", 2);

			npcStartConversation (player, npc, "naboo_imperial_tier3_barlow", message, responses);
		}
		else
		{
			chat.chat (npc, player, message);
		}

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (naboo_imperial_tier3_barlow_condition_alreadyTalkedTo (player, npc))
	{
		doAnimationAction (npc, "wave_finger_warning");

		//-- NPC: You again? I don't know anything else. I have already told you everything that I know. Now please, let me continue my duties in peace.
		string_id message = new string_id (c_stringFile, "s_f813490a");
		chat.chat (npc, player, message);

		return SCRIPT_CONTINUE;
	}

	//-- [NOTE] 
	if (naboo_imperial_tier3_barlow_condition__defaultCondition (player, npc))
	{
		doAnimationAction (npc, "wave_on_dismissing");

		//-- NPC: I do not recall requesting your presence? If you have no business here I highly suggest that you get back to your unit or where ever you came from.
		string_id message = new string_id (c_stringFile, "s_4355764f");
		chat.chat (npc, player, message);

		return SCRIPT_CONTINUE;
	}

	chat.chat (npc, "Error:  All conditions for OnStartNpcConversation were false.");

	return SCRIPT_CONTINUE;
}

