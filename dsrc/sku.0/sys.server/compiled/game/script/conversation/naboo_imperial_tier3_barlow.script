int naboo_imperial_tier3_barlow_handleBranch86 (obj_id player, obj_id npc, string_id response)
{
	//-- [BRANCH NOTE] 
	//-- NPC: Uh? Am I in some sort of trouble? You are not going to do anything to me are you? Because I will have you know that torture of an officer is forbidden under Imperial directive Y70-32C subsection 14.

	//-- [RESPONSE NOTE] 
	//-- PLAYER: There will be no need if you answer some questions for me.
	if (response == "s_f9e3a71b")
	{
		doAnimationAction (player, "explain");

		//-- [NOTE] 
		if (naboo_imperial_tier3_barlow_condition__defaultCondition (player, npc))
		{
			//-- NPC: Of course I will cooperate. It is every Imperial officer's duty to cooperate in the fullest with any Inquisition investigation. All you had to do was ask. You didn't have to threaten me like that.
			string_id message = new string_id (c_stringFile, "s_3d1204e0");
			int numberOfResponses = 0;

			boolean hasResponse = false;

			//-- PLAYER: Silence! I will ask you questions and you will answer them.
			boolean hasResponse0 = false;
			if (naboo_imperial_tier3_barlow_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse0 = true;
			}

			//-- PLAYER: Sorry, I am sort of new at this whole Inquisition thing.
			boolean hasResponse1 = false;
			if (naboo_imperial_tier3_barlow_condition__defaultCondition (player, npc))
			{
				++numberOfResponses;
				hasResponse = true;
				hasResponse1 = true;
			}

			if (hasResponse)
			{
				int responseIndex = 0;
				string_id responses [] = new string_id [numberOfResponses];

				if (hasResponse0)
					responses [responseIndex++] = new string_id (c_stringFile, "s_3700f6");

				if (hasResponse1)
					responses [responseIndex++] = new string_id (c_stringFile, "s_9d1204e3");

				setObjVar (player, "conversation.naboo_imperial_tier3_barlow.branchId", 6);

				npcSpeak (player, message);
				npcSetConversationResponses (player, responses);
			}
			else
			{
				removeObjVar (player, "conversation.naboo_imperial_tier3_barlow.branchId");

				npcSpeak (player, message);
				npcEndConversation (player);
			}

			return SCRIPT_CONTINUE;
		}

	}

	return SCRIPT_DEFAULT;
}

