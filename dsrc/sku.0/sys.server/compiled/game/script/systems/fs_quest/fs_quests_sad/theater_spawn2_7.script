include library.quests;

messageHandler OnTheaterCreated()
{
	LOG("newquests", "theater_spawn1");
	obj_id player = params.getObjId("player");
	LOG("newquests", "theater_spawn1 player = " + player);
	location mylocation = getLocation(self);
	quests.createSpawner("fs_quest_sad_2_7_1", mylocation, "datatables/quest/force_sensitive/base_spawn.iff", player);
	quests.createSpawner("fs_quest_sad_2_7_2", mylocation, "datatables/quest/force_sensitive/base_spawn.iff", player);
	quests.createSpawner("fs_quest_sad_2_7_3", mylocation, "datatables/quest/force_sensitive/base_spawn.iff", player);
	return SCRIPT_CONTINUE;
}